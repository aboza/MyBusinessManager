<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LogInServlet</servlet-name>
        <servlet-class>Servlets.LogInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOutServlet</servlet-name>
        <servlet-class>Servlets.LogOutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditVendorServlet</servlet-name>
        <servlet-class>Servlets.EditVendorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowVendorServlet</servlet-name>
        <servlet-class>Servlets.ShowVendorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditCustomerServlet</servlet-name>
        <servlet-class>Servlets.EditCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowCustomerServlet</servlet-name>
        <servlet-class>Servlets.ShowCustomerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogInServlet</servlet-name>
        <url-pattern>/LogInServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOutServlet</servlet-name>
        <url-pattern>/LogOutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditVendorServlet</servlet-name>
        <url-pattern>/EditVendorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowVendorServlet</servlet-name>
        <url-pattern>/ShowVendorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditCustomerServlet</servlet-name>
        <url-pattern>/EditCustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowCustomerServlet</servlet-name>
        <url-pattern>/ShowCustomerServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
